{
  "name": "kun-hash",
  "version": "0.0.1",
  "description": "kun-hash is a tool for quickly calculating file hash",
  "author": "https://github.com/Tkunl",
  "main": "index.js",
  "module": "output/kunHash.esm-bundler.js",
  "types": "output/types/main.d.ts",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "node": "./index.cjs",
      "default": "./output/kunHash.esm-bundler.js"
    }
  },
  "files": [
    "output",
    "index.cjs",
    "index.js",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "npm run rm:windows && rollup -cw",
    "build": "npm run rm:windows && rollup -c",
    "rm:windows": "if exist dist rmdir /s/q output"
  },
  "keywords": [
    "kun-hash",
    "hash"
  ],
  "license": "MIT",
  "dependencies": {
    "crc-32": "^1.2.2",
    "hash-wasm": "^4.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "browserslist": "^4.23.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-web-worker-loader": "^1.6.1"
  }
}
